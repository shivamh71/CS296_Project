set ylabel "Time (in milliseconds)"
set datafile separator ","
plot\
   "./data/lab05_g16_data.csv" using 1:3 smooth unique title "Step times" with points,\
   "./data/lab05_g16_data.csv" using 1:4 smooth unique title "Collision times" with points,\
   "./data/lab05_g16_data.csv" using 1:5 smooth unique title "Velocity update times" with points,\
   "./data/lab05_g16_data.csv" using 1:6 smooth unique title "Position update times" with points
